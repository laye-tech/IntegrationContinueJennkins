{
  "name": "CICD_BY_LAYE_TECH",
  "description": "Je viens de debuter l'apprentissage du devOps",
  "type": "project",
  "require": {
    "php": "^8.0",
    "laravel/framework": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    }
  },
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ]
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  }
}

